{
    "sources": [
        {
            "name": "Logstash",
            "url": "file:///var/log/logstash/logstash-plain.log"
        },
        {
            "name": "Elasticsearch",
            "url": "file:///var/log/elasticsearch/*.log",
        },
        {
            "name": "Kafka",
            "url": "file:///tmp/kafka-logs/*.log"
        }
    ],
    "route": {
        "root_path": "/logserver"
    }
}
